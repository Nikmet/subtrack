{
    "name": "subtrack",
    "version": "0.1.0",
    "private": true,
    "workspaces": [
        "server",
        "client"
    ],
    "scripts": {
        "dev": "concurrently -n server,client -c cyan,magenta \"npm run dev -w server\" \"npm run dev -w client\"",
        "build": "npm run build --workspaces --if-present",
        "lint": "npm run lint --workspaces --if-present",
        "typecheck": "npm run typecheck --workspaces --if-present",
        "test": "npm run test --workspaces --if-present",
        "legacy:dev": "next dev",
        "legacy:build": "next build",
        "legacy:start": "next start"
    },
    "dependencies": {
        "@prisma/adapter-pg": "^7.4.0",
        "@prisma/client": "^7.3.0",
        "@vercel/blob": "^2.3.0",
        "bcryptjs": "^3.0.3",
        "lucide-react": "^0.575.0",
        "next": "16.1.6",
        "next-auth": "^5.0.0-beta.30",
        "pg": "^8.18.0",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "sonner": "^2.0.7"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4",
        "@types/node": "^20",
        "@types/pg": "^8.16.0",
        "@types/react": "^19",
        "@types/react-dom": "^19",
        "concurrently": "^9.2.1",
        "eslint": "^9",
        "eslint-config-next": "16.1.6",
        "prisma": "^7.3.0",
        "tailwindcss": "^4",
        "typescript": "^5"
    },
    "ignoreScripts": [
        "sharp",
        "unrs-resolver"
    ],
    "trustedDependencies": [
        "sharp",
        "unrs-resolver"
    ]
}
